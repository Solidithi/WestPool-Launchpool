generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Project {
  id                  Int      @id @default(autoincrement())
  projectName         String
  projectOwnerAddress String
  verifiedToken       String
  projectLogo         String
  projectImage        String[]
  shortDescription    String
  longDescription     String
  acceptedVToken      Int
  minStake            Int
  maxStake            Int
  fromDate            DateTime
  toDate              DateTime
  txHashCreated       String
  projectStatus       String
}

model VTokenData {
  id Int @id @default(autoincrement())
}

model User {
  id Int @id @default(autoincrement())
}
